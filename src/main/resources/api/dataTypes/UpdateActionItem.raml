#%RAML 1.0 DataType
type: object
description: 'Defines structure of update action Item request'
#examples: !include ../examples/ActionItem.raml
properties: 
  studyIssueID:
    type: integer
    required: true
  occuranceDate:
    type: datetime
    required: true
  dateClosed:
    type: datetime
    required: false
  followUpDate:
    type: datetime
    required: false
  comments:
    type: string
    minLength: 1
    maxLength: 2000
    required: false
  studyIssueTypeName:
    type: !include StudyIssueType.raml
    required: true
  notes:
    type: string
    minLength: 1
    maxLength: 2000
    required: true
  studyIssueStatusName:
    type: string
    required: true
    minLength: 1
    maxLength: 50
    enum:
      - Closed
      - Opened
      - Resolved
  protocolId:
    type: integer
    required: true
  protocolSiteId:
    type: integer
    required: true
  recordLocked:
    type: integer
    required: false
    enum:
      - 0
      - 1
  lastUser:
    type: integer
    required: true
